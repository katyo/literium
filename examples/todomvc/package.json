{
  "name": "literium-example-todomvc",
  "version": "0.0.3",
  "description": "The TodoMVC example using literium Web-framework.",
  "scripts": {
    "compile": "ts-node src/html && rollup -c",
    "watch": "rollup -c -w",
    "clean": "rm -rf dist",
    "start": "ts-node src/server.ts start 8182",
    "stop": "ts-node src/server.ts stop 8182",
    "postinstall": "npm run compile",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/katyo/literium"
  },
  "keywords": [
    "todomvc",
    "client",
    "server",
    "literium",
    "framework",
    "example",
    "demo"
  ],
  "author": "K. <kayo@illumium.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/katyo/literium/issues"
  },
  "devDependencies": {
    "@types/node": "^9.4.6",
    "postcss-import": "^11.1.0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-gzip": "^1.3.0",
    "rollup-plugin-node-resolve": "^3.2.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-sass": "^0.7.2",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.14.0",
    "rollup-plugin-uglify": "^4.0.0",
    "rollup-plugin-visualizer": "~0.5.0"
  },
  "dependencies": {
    "literium": "^0.2.0",
    "literium-json": "^0.2.0",
    "literium-runner": "^0.2.1",
    "raf-polyfill": "^1.0.0",
    "ts-node": "^6.0.5",
    "typescript": "^2.9.1",
    "todomvc-app-css": "^2.1.2",
    "todomvc-common": "^1.0.5"
  }
}
