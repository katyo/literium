{
  "name": "literium-example-todomvc",
  "version": "0.0.3",
  "description": "The TodoMVC example using literium Web-framework.",
  "main": "index.js",
  "scripts": {
    "compile": "webpack --progress",
    "compile-dev": "webpack --verbose",
    "watch": "webpack --watch",
    "start": "ts-node src/server.ts start 8182",
    "stop": "ts-node src/server.ts stop 8182",
    "postinstall": "npm run compile",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/katyo/literium"
  },
  "keywords": [
    "virtual-dom",
    "reactive",
    "client",
    "server",
    "todomvc"
  ],
  "author": "K. <kayo@illumium.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/katyo/literium/issues"
  },
  "devDependencies": {
    "@types/compression-webpack-plugin": "^0.4.0",
    "@types/extract-text-webpack-plugin": "^3.0.0",
    "@types/node": "^9.4.6",
    "@types/webpack": "^3.8.5",
    "@types/webpack-bundle-analyzer": "^2.9.0",
    "compression-webpack-plugin": "^1.1.3",
    "css-loader": "^0.28.9",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "ts-loader": "^3.2.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "2.11.1"
  },
  "dependencies": {
    "literium": "file:../../",
    "raf-polyfill": "^1.0.0",
    "todomvc-app-css": "^2.1.0",
    "todomvc-common": "^1.0.4"
