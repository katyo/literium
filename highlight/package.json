{
  "name": "literium-highlight",
  "version": "0.2.5",
  "description": "Syntax highlighter module for Literium web-framework.",
  "main": "index.js",
  "typings": "index.d.ts",
  "module": "es/index.js",
  "scripts": {
    "compile": "npm run compile-es && npm run compile-js",
    "clean": "npm run clean-es && npm run clean-js",
    "compile-es": "tsc --outDir es --module es6",
    "clean-es": "rm -rf es",
    "compile-js": "tsc --outDir .",
    "clean-js": "rm -rf *.d.ts *.js *.js.map",
    "prepare": "npm run compile",
    "test": "exit 0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/katyo/literium/tree/master/highlight"
  },
  "keywords": [
    "hightlight",
    "markup",
    "render",
    "virtual-dom",
    "client",
    "server",
    "literium",
    "framework",
    "typescript",
    "es6",
    "es2015"
  ],
  "author": "K. <kayo@illumium.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/katyo/literium/issues"
  },
  "devDependencies": {
    "@types/node": "^8",
    "typescript": "^2.9"
  },
  "dependencies": {
    "literium": "^0.2",
    "highlight-ts": "^9.12"
  }
}
